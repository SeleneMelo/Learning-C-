#include <iostream>
using namespace std;

int main(){
    int i, j, x;
    
    std::cin >> x;
    
    for(i=1; i<=x; i++){
        for(j=1; j<=i; j++){
            std::cout << "*";
        }
        std::cout << "\n";
    }
    
    for(i=x; i>0; i--){
        for(j=1; j<i; j++){
            std::cout << "*";
        }
        std::cout << "\n";
    }
    
    return 0;
} 
